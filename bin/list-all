#!/usr/bin/env bash

set -euo pipefail

# Get all available versions from Docker's download page
curl -fsSL "https://download.docker.com/mac/static/stable/x86_64/" | \
  grep -o 'docker-[0-9]*\.[0-9]*\.[0-9]*\.tgz' | \
  sed 's/docker-\(.*\)\.tgz/\1/' | \
  sort -t. -k 1,1n -k 2,2n -k 3,3n | \
  uniq | \
  tr '\n' ' ' | \
  sed 's/ $//'

echo
